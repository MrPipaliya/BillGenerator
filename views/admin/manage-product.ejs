<body data-sidebar="dark">

    <!-- <body data-layout="horizontal" data-topbar="dark"> -->

    <!-- Begin page -->
    <div id="layout-wrapper">

        <header id="page-topbar">
            <div class="navbar-header">
                <div class="d-flex">
                    <%- include('config/header'); %>
                </div>
        </header>

        <!-- ========== Left Sidebar Start ========== -->
        <%- include('config/leftbar'); %>
            <!-- Left Sidebar End -->

            <!-- ============================================================== -->
            <!-- Start right Content here -->
            <!-- ============================================================== -->
            <div class="main-content">

                <div class="page-content">
                    <div class="container-fluid">

                        <!-- start page title -->
                        <div class="row">
                            <div class="col-12">
                                <div class="page-title-box d-sm-flex align-items-center justify-content-between">
                                    <h4 class="mb-sm-0 font-size-18">Manage Product</h4>

                                    <div class="page-title-right">
                                        <ol class="breadcrumb m-0">
                                            <li class="breadcrumb-item active">Product</li>
                                        </ol>
                                    </div>

                                </div>
                            </div>
                        </div>
                        <!-- end page title -->

                        <div class="row">

                            <div class="col-xl-4">
                                <div class="card">
                                    <div class="card-body">
                                        <h4 class="card-title mb-4">Product</h4>

                                        <form method="post" action="/admin/product/" enctype="multipart/form-data">

                                            <% if(productUpdateData.FileName){%>
                                                <div class="row mb-4">
                                                    <label for="horizontal-firstname-input"
                                                        class="col-sm-3 col-form-label">

                                                    </label>
                                                    <div class="col-sm-9">
                                                        <img src="images/productimages/<%= productUpdateData.FileName %>"
                                                            style="width:50px;height:50px;" />
                                                    </div>
                                                </div>
                                                <input type="hidden" value="<%= productUpdateData.id %>" name="id" />
                                                <input type="hidden" value="update" name="operation" />
                                                <%} %>

                                                    <div class="row mb-4">
                                                        <label for="horizontal-firstname-input"
                                                            class="col-sm-3 col-form-label">Image</label>
                                                        <div class="col-sm-9">
                                                            <input type="file" class="form-control" id="file"
                                                                name="file" <%=productUpdateData.id?"":"required" %>>
                                                        </div>
                                                    </div>
                                                    <div class="row mb-4">
                                                        <label for="horizontal-firstname-input"
                                                            class="col-sm-3 col-form-label">Product</label>
                                                        <div class="col-sm-9">
                                                            <font style="color: red;">
                                                                <%= formDataError[0].context.label %>
                                                            </font>
                                                            <input type="text" class="form-control" id="ProductName"
                                                                name="ProductName"
                                                                value="<%= productUpdateData.ProductName %>" />
                                                        </div>
                                                    </div>
                                                    <div class="row mb-4">
                                                        <label for="horizontal-firstname-input"
                                                            class="col-sm-3 col-form-label">Cost</label>
                                                        <div class="col-sm-9">
                                                            <font style="color: red;">
                                                                <%= formDataError[0].context.label %>
                                                            </font>
                                                            <input type="Number" class="form-control" id="ProductCost"
                                                                name="ProductCost"
                                                                value="<%= productUpdateData.ProductCost %>"
                                                                step="any" />
                                                        </div>
                                                    </div>
                                                    <div class="row mb-4">
                                                        <label for="horizontal-firstname-input"
                                                            class="col-sm-3 col-form-label">Sell Price</label>
                                                        <div class="col-sm-9">
                                                            <font style="color: red;">
                                                                <%= formDataError[0].context.label %>
                                                            </font>
                                                            <input type="Number" class="form-control"
                                                                id="ProductSellPrice" name="ProductSellPrice"
                                                                value="<%= productUpdateData.ProductSellPrice %>"
                                                                step="any" />
                                                        </div>
                                                    </div>
                                                    <div class="row mb-4">
                                                        <label for="horizontal-firstname-input"
                                                            class="col-sm-3 col-form-label">Category</label>
                                                        <div class="col-sm-9">
                                                            <font style="color: red;">
                                                                <%= formDataError[0].context.label %>
                                                            </font>
                                                            <select class="form-select" id="ProductCategory"
                                                                name="ProductCategory">
                                                                <option>Select</option>
                                                                <% categoryList.forEach(function(record){ %>
                                                                    <option value="<%= record._id %>"
                                                                        <%=record._id==productUpdateData.CategoryId?"selected":""
                                                                        %>>
                                                                        <%= record.CategoryName %>
                                                                    </option>
                                                                    <% }) %>
                                                            </select>
                                                        </div>
                                                    </div>
                                                    <div class="row mb-4">
                                                        <label for="horizontal-email-input"
                                                            class="col-sm-3 col-form-label">Status</label>
                                                        <div class="col-sm-9">
                                                            <div class="form-check form-switch form-switch-lg mb-3 form-check-success"
                                                                dir="ltr">
                                                                <input class="form-check-input" type="checkbox"
                                                                    id="Status" name="Status"
                                                                    <%=productUpdateData.Status?"checked":""; %>>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="row justify-content-end">
                                                        <div class="col-sm-9">
                                                            <div>
                                                                <button type="submit" class="btn btn-primary w-md">
                                                                    <%= productUpdateData.id?"Update":"Add" %>
                                                                </button>
                                                            </div>
                                                        </div>
                                                    </div>
                                        </form>
                                    </div>
                                    <!-- end card body -->
                                </div>
                                <!-- end card -->
                            </div>

                            <div class="col-xl-8">
                                <div class="card">
                                    <div class="card-body">

                                        <h4 class="card-title">Product List</h4>

                                        <table id="datatable" class="table table-bordered dt-responsive  nowrap w-100">
                                            <thead>
                                                <tr>
                                                    <th>Image</th>
                                                    <th>Name</th>
                                                    <th>Cost</th>
                                                    <th>Selling Price</th>
                                                    <th>Status</th>
                                                    <th>Action</th>
                                                </tr>
                                            </thead>

                                            <tbody>
                                                <% productList.forEach(function(record){ %>
                                                    <tr>
                                                        <td>
                                                            <img src="images/productimages/<%= record.FileName %>"
                                                                style="width:50px;height:50px;" />
                                                        </td>
                                                        <td>
                                                            <%= record.ProductName %>
                                                        </td>
                                                        <td>
                                                            <%= record.ProductCost %>
                                                        </td>
                                                        <td>
                                                            <%= record.ProductSellPrice %>
                                                        </td>
                                                        <td>
                                                            <% if(record.Status==true){ %>
                                                                <span
                                                                    class="badge badge-pill badge-soft-success font-size-11">Active</span>
                                                                <% }else{ %>
                                                                    <span
                                                                        class="badge badge-pill badge-soft-danger font-size-11">Inactive</span>
                                                                    <% } %>
                                                        </td>
                                                        <td>
                                                            <a href="/admin/product/<%= record._id %>"
                                                                class="btn btn-primary btn-sm"><span
                                                                    class="bx bx-edit"></span></a>
                                                            <a href="/admin/product/delete/<%= record._id %>"
                                                                class="btn btn-danger btn-sm"><span
                                                                    class="bx bx-trash"></span></a>
                                                        </td>
                                                    </tr>
                                                    <% }); %>
                                            </tbody>

                                        </table>

                                    </div>
                                </div>
                            </div>

                        </div>
                        <!-- end row -->

                    </div>
                    <!-- container-fluid -->
                </div>
