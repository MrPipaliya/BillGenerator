                        </div>
                        <!-- end page title -->

                        <div class="row">
                            <div class="col-xl-12">
                                <div class="card">
                                    <div class="card-body">
                                        <h4 class="card-title">Bill List</h4>

                                        <table id="datatable" class="table table-bordered dt-responsive  nowrap w-100">
                                            <thead>
                                                <tr>
                                                    <th>Order Date</th>
                                                    <th>Buyer</th>
                                                    <th>Item</th>
                                                    <th>Delete</th>
                                                </tr>
                                            </thead>

                                            <tbody>
                                                <% billList.forEach(function(record){ %>
                                                    <tr>
                                                        <td>
                                                            <!-- <%= record.Bill.ProductPrice %> -->
                                                            <!-- <%= Total = record.Bill.ProductPrice * record.Bill.Qty %> -->
                                                            <%= record.Bill.OrderDate %>
                                                        </td>
                                                        <td>
                                                            <%= record.Bill.BuyerName %>
                                                        </td>
                                                        <td>
                                                            <table
                                                                class="table table-bordered dt-responsive  nowrap w-100">
                                                                <thead>
                                                                    <tr>
                                                                        <th>Product</th>
                                                                        <th>Qty</th>
                                                                        <th>Price</th>
                                                                        <th><b>Total</b></th>
                                                                    </tr>
                                                                </thead>
                                                                <tbody>

                                                                    <% BillAmount=0; 
                                                    for (let index=0; index < record.Bill.Product.length; index++) {
                                                                        BillAmount+=record.Bill.TotalAmount[index]; %>
                                                                        <tr>
                                                                            <td>
                                                                                <%= record.Bill[0] %>
                                                                                <%=record.Bill.Product[index] %>
                                                                            </td>
                                                                            <td>
                                                                                <%= record.Bill.Qty[index] %>
                                                                            </td>
                                                                            <td>
                                                                                <%= record.Bill.ProductPrice[index] %>
                                                                            </td>
                                                                            <td><b>
                                                                                 <%= record.Bill.TotalAmount[index] %> <!- -->
                                                                            </td></b>
                                                                        </tr>

                                                                        <% } %>

                                                                            <tr>
                                                                                <td>
                                                                                </td>
                                                                                <td>
                                                                                </td>
                                                                                <td><b>Total</b>
                                                                                </td>
                                                                                <td><b>
                                                                                        <%= BillAmount  %>
                                                                                    </b> <!--Total -->
                                                                                </td>
                                                                            </tr>

                                                                </tbody>
                                                            </table>


                                                        </td>
                                                        <td style="text-align: center;width:10px;">
                                                            <a href="/admin/bill/delete/<%= record._id %>"
                                                                class="btn btn-danger btn-sm"><span
                                                                    class="bx bx-trash"></span></a>
                                                        </td>

                                                    </tr>
                                                    <% }); %>
                                            </tbody>

                                        </table>

                                    </div>
                                </div>
                            </div>

                        </div>
                        <!-- end row -->

                    </div>
                    <!-- container-fluid -->
                </div>
